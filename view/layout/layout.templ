package layout

templ Layout(childrens templ.Component) {
  <!DOCTYPE html>
  <html>
    <head>
      <script src="https://unpkg.com/htmx.org@2.0.4"></script>
      <script src="https://cdn.tailwindcss.com"></script>
    </head>
    <body class="flex min-h-screen bg-gray-100">
      @childrens
    </body>
  </html>
}
 

